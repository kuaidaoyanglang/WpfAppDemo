<Window x:Class="WpfAppCommand.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppCommand"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel x:Name="stackPanel">
        <Button x:Name="button1" Margin="5" Content="Send Command" FontSize="40"></Button>
        <TextBox x:Name="txt1" Margin="5" FontSize="40" Height="340"></TextBox>
    </StackPanel>
</Window>
